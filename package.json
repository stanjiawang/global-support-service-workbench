{
  "name": "global-support-service-workbench",
  "version": "0.1.0",
  "private": true,
  "description": "Global Support & Service Workbench monorepo scaffold",
  "packageManager": "pnpm@10.6.0",
  "scripts": {
    "dev": "pnpm --filter @apps/support-workbench-web run dev",
